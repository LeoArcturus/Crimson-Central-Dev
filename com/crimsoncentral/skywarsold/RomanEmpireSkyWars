	public RomanEmpireSkyWars() {
	}

	@EventHandler
	public void blockChestInterract(PlayerInteractEvent event) {
		Player p = event.getPlayer();
		if(deadPlayers.contains(p))
		{
	    	if(event.getAction() == Action.RIGHT_CLICK_BLOCK) {
	    		if(event.getClickedBlock().getType().equals(Material.CHEST))
	    		{
	    			event.setCancelled(true);
	    		}
	    	}
		}
	}
	
	public void cageDelete()
	{
		World world = Bukkit.getWorld("RomanEmpire");
		Location loc1 = new Location(world, 20, 130, -60);
		Location loc2 = new Location(world, -20, 130, -60);
		Location loc3 = new Location(world, -60, 130, -60);
		Location loc4 = new Location(world, -60, 130, -20);
		Location loc5 = new Location(world, -60, 130, 20);
		Location loc6 = new Location(world, -60, 130, 60);
		Location loc7 = new Location(world, -20, 130, 60);
		Location loc8 = new Location(world, 20, 130, 60);
		Location loc9 = new Location(world, 60, 130, 60);
		Location loc10 = new Location(world, 60, 130, 20);
		Location loc11 = new Location(world, 60, 130, -20);
		Location loc12 = new Location(world, 60, 130, -60);
		
		Block block1 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc1);
		block1.setType(Material.AIR);
		Block block2 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc2);
		block2.setType(Material.AIR);
		Block block3 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc3);
		block3.setType(Material.AIR);
		Block block4 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc4);
		block4.setType(Material.AIR);
		Block block5 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc5);
		block5.setType(Material.AIR);
		Block block6 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc6);
		block6.setType(Material.AIR);
		Block block7 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc7);
		block7.setType(Material.AIR);
		Block block8 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc8);
		block8.setType(Material.AIR);
		Block block9 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc9);
		block9.setType(Material.AIR);
		Block block10 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc10);
		block10.setType(Material.AIR);
		Block block11 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc11);
		block11.setType(Material.AIR);
		Block block12 = Bukkit.getWorld("RomanEmpire").getBlockAt(loc12);
		block12.setType(Material.AIR);
	}
	
